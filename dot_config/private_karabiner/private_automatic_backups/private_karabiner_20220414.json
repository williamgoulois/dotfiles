                "to": [
                  {
                    "key_code": "down_arrow"
                  }
                ],
                "type": "basic"
              },
              {
                "from": {
                  "key_code": "l",
                  "modifiers": {
                    "mandatory": [
                      "left_shift",
                      "left_command",
                      "left_control",
                      "left_option"
                    ],
                    "optional": ["any"]
                  }
