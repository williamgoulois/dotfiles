{
  "segments": [
    "username",
    "hostname",
    "virtual_env",
    "ssh",
    "cwd",
    "git",
    "hg",
    "jobs",
    "newline",
    "root"
  ],
  "mode": "patched",
  "cwd": {
    "max_depth": 4
  },
  "theme": "~/.config/powerline-shell/themes/basic.py",
  "vcs": {
    "show_symbol": "true"
  }
}
