    "args": {
      "sequence": [
        "whichkey.show",
        { "command": "whichkey.triggerKey", "args": { "key": "\\" } }
      ]
    },
    "when": "neovim.init && activeViewlet != 'workbench.view.extensions' !inSettingsEditor && !inQuickOpen && !searchInputBoxFocus && !inKeybindingsSearch && !terminalFocus && neovim.mode != 'visual' && neovim.mode !='insert'",
  },
